{
    "name": "anibalealvarezs/apis-hub",
    "type": "project",
    "authors": [
        {
            "name": "Aníbal Álvarez",
            "email": "anibalealvarezs@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "anibalealvarezs/api-client-skeleton": "@dev",
        "anibalealvarezs/amazon-api": "@dev",
        "anibalealvarezs/facebook-graph-api": "@dev",
        "anibalealvarezs/google-api": "@dev",
        "anibalealvarezs/klaviyo-api": "@dev",
        "anibalealvarezs/netsuite-api": "@dev",
        "anibalealvarezs/shopify-api": "@dev",
        "anibalealvarezs/triple-whale-api": "@dev",
        "doctrine/orm": "^2.12.3",
        "doctrine/dbal": "^3.3.7",
        "doctrine/annotations": "1.13.2",
        "fakerphp/faker": "^1.20",
        "nesbot/carbon": "^2.61",
        "symfony/yaml": "^5.4.10",
        "symfony/config": "^6.1",
        "symfony/routing": "^6.1",
        "symfony/http-foundation": "^6.1",
        "symfony/http-kernel": "^6.1",
        "symfony/cache": "^6.1",
        "symfony/console": "^6.0",
        "symfony/redis-messenger": "5.4.x-dev",
        "symfony/cache-contracts": "3.6.x-dev",
        "predis/predis": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.5.3",
        "phpbench/phpbench": "^1.2.6",
        "phpunit/phpunit": "^9",
        "mikey179/vfsstream": "2.0.x-dev"
    },
    "repositories": [
        {
            "type": "composer", "url": "https://satis.anibalalvarez.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": false
}
